daemonize yes
dir /var/lib/redis/
pidfile /var/run/redis/redis-server.pid
logfile /var/log/redis/redis-server.log

port 6379
bind {{redis_bind_address}}
timeout 300

loglevel notice

## Default configuration options
databases 16

save 900 1
save 300 10
save 60 10000

rdbcompression yes
dbfilename dump.rdb

appendonly no
